{
	"name": "kal-chat",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Kaleab S. Melkie",
		"email": "hi@kaleabmelkie.com",
		"url": "https://kaleabmelkie.com"
	},
	"engines": {
		"node": "^20",
		"pnpm": "^9"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky",
		"sync": "svelte-kit sync",
		"dev": "svelte-kit sync && vite dev --port 5173",
		"build": "svelte-kit sync && vite build && node scripts/update-vercel-config",
		"preview": "vite preview --port 5173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --check --write . && eslint --fix .",
		"db:check": "drizzle-kit check:sqlite",
		"db:push": "drizzle-kit push:sqlite",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@auth/sveltekit": "0.15.0",
		"@builder.io/partytown": "0.10.1",
		"@libsql/client": "0.6.0",
		"@sentry/node": "7.110.1",
		"@sentry/svelte": "7.110.1",
		"@sentry/tracing": "7.110.1",
		"@sveltejs/adapter-vercel": "5.3.0",
		"@sveltejs/kit": "2.5.6",
		"@sveltejs/vite-plugin-svelte": "3.1.0",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.12",
		"@types/eslint": "8.56.9",
		"@types/node": "^20",
		"@types/nprogress": "0.2.3",
		"@types/sanitize-html": "2.11.0",
		"@typescript-eslint/eslint-plugin": "7.7.0",
		"@typescript-eslint/parser": "7.7.0",
		"autoprefixer": "10.4.19",
		"bowser": "2.11.0",
		"dayjs": "1.11.10",
		"dotenv": "16.4.5",
		"drizzle-kit": "0.20.14",
		"drizzle-orm": "0.30.8",
		"drizzle-zod": "0.5.1",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.37.0",
		"gpt3-tokenizer": "1.1.5",
		"highlight.js": "11.9.0",
		"husky": "9.0.11",
		"marked": "12.0.1",
		"marked-highlight": "2.1.1",
		"marked-mangle": "1.1.7",
		"marked-smartypants": "1.1.6",
		"nprogress": "0.2.0",
		"openai-edge": "1.2.2",
		"postcss": "8.4.38",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.3",
		"prettier-plugin-tailwindcss": "0.5.14",
		"rollup-plugin-visualizer": "5.12.0",
		"sanitize-html": "2.13.0",
		"svelte": "4.2.15",
		"svelte-check": "3.6.9",
		"tailwindcss": "3.4.3",
		"tslib": "2.6.2",
		"typescript": "5.4.5",
		"vite": "5.2.9",
		"zod": "3.22.4"
	},
	"type": "module",
	"packageManager": "pnpm@9.0.2+sha256.d6fc013639b81658ff175829ebb9435bcb89eff206769e460fd3ae27c2054df6"
}
