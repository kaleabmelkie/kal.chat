{
	"name": "kal-chat",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Kaleab S. Melkie",
		"email": "hi@kaleabmelkie.com",
		"url": "https://kaleabmelkie.com"
	},
	"engines": {
		"node": "^18",
		"pnpm": "^8"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky install",
		"sync": "svelte-kit sync",
		"dev": "prisma generate && svelte-kit sync && vite dev --port 5173 --open",
		"build": "prisma generate && svelte-kit sync && vite build && node scripts/update-vercel-config",
		"preview": "vite preview --port 5173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --check --write . && eslint --fix .",
		"prisma": "prisma",
		"prisma:gen": "prisma generate",
		"prisma:pull": "prisma db pull",
		"prisma:push": "prisma db push",
		"prisma:studio": "prisma studio"
	},
	"devDependencies": {
		"@auth/core": "0.10.0",
		"@auth/sveltekit": "0.3.6",
		"@builder.io/partytown": "0.8.0",
		"@prisma/client": "5.0.0",
		"@sentry/node": "7.59.3",
		"@sentry/svelte": "7.59.3",
		"@sentry/tracing": "7.59.3",
		"@sveltejs/adapter-vercel": "3.0.2",
		"@sveltejs/kit": "1.22.3",
		"@tailwindcss/forms": "0.5.4",
		"@tailwindcss/typography": "0.5.9",
		"@types/marked": "4.3.1",
		"@types/node": "^18",
		"@types/nprogress": "0.2.0",
		"@types/sanitize-html": "2.9.0",
		"@typescript-eslint/eslint-plugin": "6.1.0",
		"@typescript-eslint/parser": "6.1.0",
		"autoprefixer": "10.4.14",
		"bowser": "2.11.0",
		"dayjs": "1.11.9",
		"eslint": "8.45.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.32.2",
		"gpt3-tokenizer": "1.1.5",
		"highlight.js": "11.8.0",
		"husky": "8.0.3",
		"marked": "4.3.0",
		"nprogress": "0.2.0",
		"openai-edge": "1.2.0",
		"postcss": "8.4.26",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.4.1",
		"prisma": "5.0.0",
		"rollup-plugin-visualizer": "5.9.2",
		"sanitize-html": "2.11.0",
		"svelte": "4.1.0",
		"svelte-check": "3.4.6",
		"tailwindcss": "3.3.3",
		"tslib": "2.6.0",
		"typescript": "5.1.6",
		"vite": "4.4.4"
	},
	"type": "module"
}
