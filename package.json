{
	"name": "kal-chat",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Kaleab S. Melkie",
		"email": "hi@kaleabmelkie.com",
		"url": "https://kaleabmelkie.com"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"sync": "svelte-kit sync",
		"dev": "prisma generate && svelte-kit sync && vite dev",
		"build": "prisma generate && svelte-kit sync && vite build",
		"preview": "vite preview",
		"test": "playwright test --pass-with-no-tests",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --check --write . && eslint --fix .",
		"prisma": "prisma",
		"prisma:gen": "prisma generate",
		"prisma:pull": "prisma db pull",
		"prisma:push": "prisma db push",
		"prisma:studio": "prisma studio"
	},
	"devDependencies": {
		"@auth/core": "0.5.1",
		"@auth/sveltekit": "0.3.0",
		"@builder.io/partytown": "0.7.6",
		"@playwright/test": "1.32.1",
		"@prisma/client": "4.12.0",
		"@sveltejs/adapter-vercel": "2.4.1",
		"@sveltejs/kit": "1.14.0",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.9",
		"@types/marked": "4.0.8",
		"@types/sanitize-html": "2.9.0",
		"@typescript-eslint/eslint-plugin": "5.57.0",
		"@typescript-eslint/parser": "5.57.0",
		"autoprefixer": "10.4.14",
		"bowser": "2.11.0",
		"dayjs": "1.11.7",
		"eslint": "8.37.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"gpt3-tokenizer": "1.1.5",
		"highlight.js": "11.7.0",
		"marked": "4.3.0",
		"openai": "3.2.1",
		"postcss": "8.4.21",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.7",
		"prettier-plugin-svelte": "2.10.0",
		"prettier-plugin-tailwindcss": "0.2.6",
		"prisma": "4.12.0",
		"rollup-plugin-visualizer": "5.9.0",
		"sanitize-html": "2.10.0",
		"svelte": "3.57.0",
		"svelte-check": "3.1.4",
		"tailwindcss": "3.3.0",
		"tslib": "2.5.0",
		"typescript": "5.0.2",
		"vite": "4.2.1"
	},
	"type": "module"
}
