{
	"name": "kal-chat",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Kaleab S. Melkie",
		"email": "hi@kaleabmelkie.com",
		"url": "https://kaleabmelkie.com"
	},
	"engines": {
		"node": "^18",
		"pnpm": "^8"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"prepare": "husky",
		"sync": "svelte-kit sync",
		"dev": "svelte-kit sync && vite dev --port 5173",
		"build": "svelte-kit sync && vite build && node scripts/update-vercel-config",
		"preview": "vite preview --port 5173",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --check --write . && eslint --fix .",
		"db:check": "drizzle-kit check:sqlite",
		"db:push": "drizzle-kit push:sqlite",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@auth/sveltekit": "0.15.0",
		"@builder.io/partytown": "0.10.1",
		"@libsql/client": "0.6.0",
		"@sentry/node": "7.110.1",
		"@sentry/svelte": "7.110.1",
		"@sentry/tracing": "7.110.1",
		"@sveltejs/adapter-vercel": "3.1.0",
		"@sveltejs/kit": "1.30.4",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.12",
		"@types/marked": "4.3.2",
		"@types/node": "^18",
		"@types/nprogress": "0.2.3",
		"@types/sanitize-html": "2.11.0",
		"@typescript-eslint/eslint-plugin": "7.7.0",
		"@typescript-eslint/parser": "7.7.0",
		"autoprefixer": "10.4.19",
		"bowser": "2.11.0",
		"dayjs": "1.11.10",
		"dotenv": "16.4.5",
		"drizzle-kit": "0.20.14",
		"drizzle-orm": "0.30.8",
		"drizzle-zod": "0.5.1",
		"eslint": "8.57.0",
		"eslint-config-prettier": "8.10.0",
		"eslint-plugin-svelte": "2.37.0",
		"gpt3-tokenizer": "1.1.5",
		"highlight.js": "11.9.0",
		"husky": "9.0.11",
		"marked": "4.3.0",
		"nprogress": "0.2.0",
		"openai-edge": "1.2.2",
		"postcss": "8.4.38",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.4.1",
		"rollup-plugin-visualizer": "5.12.0",
		"sanitize-html": "2.13.0",
		"svelte": "4.2.14",
		"svelte-check": "3.6.9",
		"tailwindcss": "3.4.3",
		"tslib": "2.6.2",
		"typescript": "5.4.5",
		"vite": "4.5.3",
		"zod": "3.22.4"
	},
	"type": "module",
	"packageManager": "pnpm@8.15.4+sha256.cea6d0bdf2de3a0549582da3983c70c92ffc577ff4410cbf190817ddc35137c2"
}
